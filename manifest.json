
{
  "description": "Performs basic login",
  "manifest_version": 2,
  "name": "GoLock3r",
  "version": "2.0",
  "homepage_url": "https://github.com/GoLock3r",
  "icons": {
    "48": "icons/key-48.png"
  },

  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "54.0a1"
    }
  },

  "background": {
    "scripts": ["storage.js"]
  },

  "options_ui": {
    "page": "choose_login/choose_login.html"
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    ""
  ]
}
